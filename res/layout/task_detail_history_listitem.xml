<?xml version="1.0" encoding="utf-8"?>

<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
             android:layout_width="fill_parent"
             android:layout_height="fill_parent"
             android:background="@android:color/white"
             android:clickable="true"
             android:focusable="true">
    <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
        <TextView
                android:id="@+id/hist_queue_time_lable"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0.5"
                android:textColor="@android:color/black"
                android:padding="5dp"
                android:text="Queue Time"/>

        <TextView
                android:id="@+id/hist_queue_time"
                android:layout_weight="0.5"
                android:layout_width="fill_parent"
                android:textColor="@android:color/black"
                android:layout_height="wrap_content"/>
    </TableRow>
    <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
        <TextView
                android:id="@+id/hist_start_time_lable"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0.5"
                android:textColor="@android:color/black"
                android:padding="5dp"
                android:text="Start Time"/>

        <TextView
                android:id="@+id/hist_start_time"
                android:layout_gravity="center"
                android:layout_weight="0.5"
                android:layout_width="fill_parent"
                android:textColor="@android:color/black"
                android:layout_height="wrap_content"/>
    </TableRow>
    <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
        <TextView
                android:id="@+id/hist_end_time_lable"
                android:layout_weight="0.5"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:textColor="@android:color/black"
                android:padding="5dp"
                android:text="End Time"/>
        <TextView
                android:id="@+id/hist_end_time"
                android:layout_weight="0.5"
                android:layout_gravity="center"
                android:layout_width="fill_parent"
                android:textColor="@android:color/black"
                android:layout_height="wrap_content"/>
    </TableRow>
    <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
        <TextView
                android:id="@+id/hist_duration_lable"
                android:layout_weight="0.5"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:textColor="@android:color/black"
                android:padding="5dp"
                android:text="Duration"/>
        <TextView
                android:id="@+id/hist_duration"
                android:layout_weight="0.5"
                android:layout_gravity="center"
                android:layout_width="fill_parent"
                android:textColor="@android:color/black"
                android:layout_height="wrap_content"/>
    </TableRow>
</TableLayout>